@masterthesis{Harmon2013,
  abstract = {Urban microsimulators have been increasingly used to forecast land use, environmental and transportation conditions in cities and are a major tool for stakeholders to analyze the effects of urban policy. Although demographic and residential land use models have been well developed for the majority of these systems, labour market conditions and the forecasting of future jobs have at best been limited to high-level exogenous processes. This thesis aims to develop and implement a truly endogenous job supply and job matching model for use with the Integrated Land Use, Transportation, Environment (ILUTE) modelling system. Jobs within the system are tracked individually at both the occupational and industrial level, and matching occurs within an open market framework in an effort to simulate the true macroeconomic conditions of the real world. iii Adam Harmon MASc Thesis Acknowledgments},
  author   = {Adam Harmon},
  title    = {A Microsimulated Industrial and Occupation-Based Labour Market Model for Use in the Integrated Land Use, Transportation, Environment (ILUTE) Modelling System},
  year     = 2013
}

