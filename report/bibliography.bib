@article{Gunderson1979,
   abstract = {[Based on data from the 1971 Canadian Census, separate earnings equations were estimated for public and private sector workers, and the over-all average earnings differential was decompsed into two parts: a portion due to different endowments of productivity-related characteristics and a portion due to a pure surplus payment for the same characteristics. The pure surplus or economic rent that public sector workers received relative to their private sector counterparts was $492, or 6.2 per cent, for males and $383, or 8.6 per cent, for females. The public sector wage advantage was larger for low-wage workers, illustrating the basic dilemma that policies to curb the advantage may conflict with the desire to raise the wages of low-wage workers and achieve equal pay for equal work between males and females. /// Les différentiels de salaire entre le secteur public et le secteur privé. A partir des données du recensement de 1971, l'auteur calibre des équations de salaires pour les travailleurs du secteur public et pour les travailleurs du secteur privé. Il décompose le différentiel de salaire moyen en deux portions: l'une attribuable à des différentiels de dotation de caractéristiques ayant un effet de productivité et l'autre attribuable à une rente économique dévolue aux porteurs de ces mêmes caractéristiques. La rente économique que recoivent les travailleurs du secteur public par rapport à leurs collègues du secteur privé est de l'ordre de $492 (soit 6.2 pour cent) pour les hommes et de $383 (soit 8.6 pour cent les femmes). Le différentiel de salaire en faveur du secteur public est plus élevé pour les travailleurs à plus bas salaire. Voilà qui met en lumière le dilemme de politiques qui cherchent en même temps à réduire l'écart secteur public/secteur privé tout en voulant promouvoir un taux de rémunération plus élevé pour les travailleurs les moins bien payés et une plus grande égalité de rémunération entre hommes et femmes pour des tâches semblables.]},
   author = {Morley Gunderson},
   doi = {10.2307/134598},
   issn = {00084085, 15405982},
   issue = {2},
   journal = {The Canadian Journal of Economics / Revue canadienne d'Economique},
   pages = {228-242},
   publisher = {[Wiley, Canadian Economics Association]},
   title = {Earnings Differentials between the Public and Private Sectors},
   volume = {12},
   url = {http://www.jstor.org/stable/134598},
   year = {1979},
}

