   keywords = {IATBR,ILUTE,International conference on travel behaviour research,Microsimulation,Modelling,Transportation and land use,Urban systems},
   month = {6},
   pages = {217-234},
   title = {ILUTE: An operational prototype of a comprehensive microsimulation model of urban systems},
